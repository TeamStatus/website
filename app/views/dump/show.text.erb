<% # -*- coding: UTF-8 -*- %>
BEGIN;
<% @users.each do |user| %>
INSERT INTO teams (name) VALUES('<%= user.email %>');
INSERT INTO users (email, full_name, calling_name, created_at, confirmed_at, team_id, type) VALUES('<%= user.email %>', '<%= user.fullName.force_encoding('utf-8') %>', '<%= user.callingName.force_encoding('utf-8') %>', '<%= user.created_at %>', '<%= user.created_at %>', (SELECT id FROM teams WHERE name='<%= user.email %>'), 'User');
	<% user.boards.each do |board| %>
		INSERT INTO boards (name, user_id, public_id) VALUES('<%= board.name %>', (SELECT id FROM users WHERE email='<%= user.email %>'), '<%= board.publicId %>');
		<% board.jobs.each do |job| %>
			INSERT INTO jobs (board_id, jobId, settings, widgetSettings) VALUES((SELECT id FROM boards WHERE public_id='<%= board.publicId %>'), '<%= job.jobId %>', '<%= job.settings.to_yaml.to_s.gsub('\'', '\\\'') %>', '<%= job.widgetSettings.to_yaml.to_s.gsub('\'', '\\\'') %>');
		<% end %>
	<% end %>
<% end %>